        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/walking" android:src="@drawable/tag_walk"></ImageView>
        <LinearLayout android:layout_height="match_parent" android:layout_width="wrap_content" android:id="@+id/linearLayout4" android:orientation="vertical">
