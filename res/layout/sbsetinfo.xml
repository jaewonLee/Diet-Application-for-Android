<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:orientation="vertical"
	android:layout_height="match_parent" android:background="@drawable/bg">
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:orientation="horizontal" android:id="@+id/sbsetinfo_Ly1" android:paddingTop="40dp">
		<LinearLayout android:layout_height="match_parent"
			android:layout_width="wrap_content" android:layout_weight="1"
			android:orientation="vertical" android:id="@+id/sbsetinfo_Ly4">
			<ImageView android:src="@drawable/info_id" android:id="@+id/sbsetinfo_imageView1"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:layout_marginLeft="20dp"></ImageView>
			<EditText android:layout_weight="1" android:hint="영문, 숫자 가능"
				android:layout_marginLeft="20dp" android:id="@+id/sbsetinfo_editText1"
				android:layout_height="wrap_content" android:layout_width="wrap_content">
				<requestFocus></requestFocus>
			</EditText>
		</LinearLayout>
		<LinearLayout android:layout_height="match_parent"
			android:layout_width="wrap_content" android:layout_weight="1"
			android:orientation="vertical" android:id="@+id/sbsetinfo_Ly5">
			<ImageView android:src="@drawable/info_gender" android:id="@+id/sbsetinfo_imageView2"
				android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
			<RadioGroup android:id="@+id/sbsetinfo_radioGroup1"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:orientation="horizontal">
				<RadioButton android:textColor="#000000"
					android:layout_height="wrap_content" android:checked="true"
					android:text="남자" android:id="@+id/sbsetinfo_man"
					android:layout_width="wrap_content"></RadioButton>
				<RadioButton android:textColor="#000000"
					android:layout_height="wrap_content" android:text="여자"
					android:id="@+id/sbsetinfo_woman" android:layout_width="wrap_content"></RadioButton>
			</RadioGroup>
		</LinearLayout>
	</LinearLayout>

	<LinearLayout android:paddingLeft="10px"
		android:paddingBottom="10px" android:layout_height="wrap_content"
		android:layout_gravity="center"
		android:layout_width="wrap_content" android:id="@+id/sbsetinfo_Ly2" android:paddingTop="10dp">
		<TableLayout android:id="@+id/tableLayout1"
			android:paddingBottom="10px" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:layout_gravity="center">
			<TableRow android:layout_gravity="center" android:id="@+id/tableRow1"
				android:layout_height="wrap_content" android:layout_width="wrap_content">
				<RelativeLayout android:id="@+id/relativeLayout2" android:layout_height="match_parent" android:layout_width="match_parent">
					<ImageView android:layout_height="wrap_content" android:id="@+id/sbsetinfo_imageView3" android:src="@drawable/info_age" android:layout_width="wrap_content"></ImageView>
					<ImageView android:layout_height="wrap_content" android:id="@+id/sbsetinfo_imageView4" android:src="@drawable/info_height" android:layout_width="wrap_content" android:layout_toRightOf="@+id/sbsetinfo_imageView3" android:layout_marginLeft="20dp"></ImageView>
					<ImageView android:layout_height="wrap_content" android:id="@+id/sbsetinfo_imageView5" android:src="@drawable/info_weight" android:layout_width="wrap_content" android:layout_toRightOf="@+id/sbsetinfo_imageView4" android:layout_marginLeft="35dp"></ImageView>
				</RelativeLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<RelativeLayout android:id="@+id/relativeLayout3" android:layout_width="wrap_content" android:layout_height="wrap_content">
					<Button android:id="@+id/sbsetinfo_Btn1" android:layout_width="75dp" android:layout_height="wrap_content" android:background="@drawable/picker_up"></Button>
					<Button android:layout_height="wrap_content" android:id="@+id/sbsetinfo_Btn2" android:layout_toRightOf="@+id/sbsetinfo_Btn1" android:layout_marginLeft="25dp" android:layout_width="75dp" android:background="@drawable/picker_up"></Button>
					<Button android:layout_height="wrap_content" android:id="@+id/sbsetinfo_Btn3" android:layout_toRightOf="@+id/sbsetinfo_Btn2" android:layout_width="75dp" android:layout_marginLeft="30dp" android:background="@drawable/picker_up"></Button>
				</RelativeLayout>
			</TableRow>
			<TableRow android:layout_gravity="center" android:id="@+id/tableRow2"
				android:layout_height="wrap_content" android:layout_width="wrap_content">
				<RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="fill_parent" android:layout_height="fill_parent">
					<EditText android:layout_height="wrap_content" android:inputType="number" android:id="@+id/sbsetinfo_Age" android:layout_width="75dp" android:maxLength="3" android:gravity="center" android:textSize="25dp"></EditText>
					<TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="세" android:textColor="#000000" android:layout_toRightOf="@+id/sbsetinfo_Age" android:layout_centerVertical="true" android:id="@+id/sbsetinfo_Tv1"></TextView>
					<EditText android:id="@+id/sbsetinfo_Height" android:layout_height="wrap_content" android:layout_toRightOf="@+id/sbsetinfo_Age" android:layout_width="75dp" android:layout_marginLeft="25dp" android:inputType="number" android:maxLength="3" android:gravity="center" android:textSize="25dp"></EditText>
					<TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/sbsetinfo_Tv2" android:text="cm" android:layout_centerVertical="true" android:layout_toRightOf="@+id/sbsetinfo_Height" android:textColor="#000000"></TextView>
					<EditText android:layout_height="wrap_content" android:id="@+id/sbsetinfo_Weight" android:layout_toRightOf="@+id/sbsetinfo_Height" android:layout_width="75dp" android:layout_marginLeft="30dp" android:inputType="number" android:maxLength="3" android:gravity="center" android:textSize="25dp"></EditText>
					<TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/sbsetinfo_Tv3" android:text="kg" android:layout_centerVertical="true" android:layout_toRightOf="@+id/sbsetinfo_Weight" android:textColor="#000000"></TextView>
				</RelativeLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<RelativeLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/RelativeLayout01">
					<Button android:layout_height="wrap_content" android:layout_width="75dp" android:layout_marginLeft="25dp" android:id="@+id/sbsetinfo_Btn5" android:layout_toRightOf="@+id/sbsetinfo_Btn4" android:background="@drawable/picker_down"></Button>
					<Button android:layout_height="wrap_content" android:layout_width="75dp" android:layout_marginLeft="30dp" android:id="@+id/sbsetinfo_Btn6" android:layout_toRightOf="@+id/sbsetinfo_Btn5" android:background="@drawable/picker_down"></Button>
					<Button android:layout_width="75dp" android:background="@drawable/picker_down" android:id="@+id/sbsetinfo_Btn4" android:layout_height="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentLeft="true"></Button>
				</RelativeLayout>
			</TableRow>
		</TableLayout>

	</LinearLayout>
	<LinearLayout android:orientation="vertical" android:layout_width="match_parent"
		android:layout_height="match_parent" android:id="@+id/sbsetinfo_Ly3" android:paddingTop="10dp">
		<ImageView android:src="@drawable/info_act"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:id="@+id/sbsetinfo_imageView6"></ImageView>
		<LinearLayout android:layout_gravity="center"
			android:paddingRight="10px" android:layout_height="wrap_content"
			android:layout_width="match_parent" android:paddingLeft="10px" android:id="@+id/sbsetinfo_Ly6">
			<SeekBar android:id="@+id/seekBar1" android:layout_weight="1"
				android:layout_height="wrap_content" android:layout_width="match_parent"></SeekBar>
		</LinearLayout>
		<LinearLayout android:layout_gravity="center"
			android:paddingBottom="20px" android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:paddingLeft="10px"
			android:orientation="vertical" android:id="@+id/sbsetinfo_Ly7">
			<ImageView android:src="@drawable/act_bar_male" android:id="@+id/imageView1"
				android:layout_height="wrap_content" android:layout_width="wrap_content"></ImageView>
			<TextView android:layout_gravity="center"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:id="@+id/sbsetinfo_textView1"></TextView>
		</LinearLayout>
		<LinearLayout android:layout_gravity="center"
			android:layout_width="match_parent"
			android:orientation="vertical" android:id="@+id/sbsetinfo_Ly8" android:gravity="bottom" android:layout_height="match_parent">
			<Button android:layout_height="wrap_content" android:text="목표설정하기" android:id="@+id/button1" android:layout_width="match_parent"></Button>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>